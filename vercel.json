{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    },
    {
      "src": "reserve/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/hello.js": {
      "maxDuration": 10
    },
    "api/test.js": {
      "maxDuration": 10
    },
    "api/simple-reservation.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/reserve",
      "dest": "/reserve/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
