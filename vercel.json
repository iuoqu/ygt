{
  "version": 2,
  "functions": {
    "api/hello.js": {
      "maxDuration": 10
    },
    "api/test.js": {
      "maxDuration": 10
    },
    "api/simple-reservation.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/reserve",
      "dest": "/public/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
